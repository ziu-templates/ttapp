{
  "presets": [
    "minify",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": [
            "Android >= 4",
            "iOS >= 10",
            "> 0.25%, not dead"
          ],
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": false,
        "regenerator": true
      }
    ]
  ],
  "comments": false
}
